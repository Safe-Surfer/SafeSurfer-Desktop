#!/usr/bin/env bash

_sscli()
{
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"

    if [[ ${cur} == -* ]] ; then
        COMPREPLY=( $(compgen -W "enable disable launch help" -- ${cur}) )
        return 0
    fi
}

complete -F _sscli sscli
