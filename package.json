{
  "name": "SafeSurfer-Desktop",
  "version": "1.0.0",
  "description": "The SafeSurfer desktop app",
  "main": "main.js",
  "license": "GPL-3.0",
  "scripts": {
    "start": "electron .",
    "sscli": "node ./assets/scripts/sscli.js",
    "electron-builder": "node_modules/.bin/electron-builder",
    "nexe": "node_modules/.bin/nexe",
    "package-linux": "electron-packager . SafeSurfer-Desktop --overwrite --asar --platform=linux --arch=x64 --icon=assets/media/icons/linux/ss-logo.png --prun=true --out=release-builds",
    "package-macos": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune --out=release-builds",
    "package-win": "electron-packager . SafeSurfer-Desktop --overwrite --asar --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"SafeSurfer-Desktop\"",
    "package-win32": "electron-packager . SafeSurfer-Desktop --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"SafeSurfer-Desktop\"",
    "lintTranslations": "node ./unitTests/lintTranslations.js",
    "lintTranslationsAll": "for i in $(command ls ./assets/translations/.); do npm run lintTranslations $(echo -e \"$i\" | cut -d'.' -f1); done"
  },
  "repository": "https://gitlab.com/safesurfer/SafeSurfer-Desktop",
  "keywords": [
    "SafeSurfer",
    "safe surfer",
    "safe",
    "surfer"
  ],
  "author": "Safe Surfer",
  "devDependencies": {
    "electron": "^2.0.8",
    "electron-builder": "^20.28.2",
    "electron-icon-maker": "0.0.4",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^12.1.1"
  },
  "dependencies": {
    "bonjour": "^3.5.0",
    "connectivity": "^1.0.1",
    "electron-download-manager": "^2.2.0",
    "electron-store": "^2.0.0",
    "electron-sudo": "^4.0.12",
    "is-admin": "^2.1.1",
    "lets-get-meta": "^2.1.1",
    "moment": "^2.22.2",
    "network": "^0.4.1",
    "nexe": "^2.0.0-rc.34",
    "node-dns-sd": "^0.2.0",
    "node_dns_changer": "^1.0.5",
    "nodejs-base64-encode": "0.0.2",
    "npm": "^6.4.0",
    "os-locale": "^3.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "shelljs": "^0.8.2",
    "store": "^2.0.12"
  }
}
