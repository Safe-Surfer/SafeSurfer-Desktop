{
  "name": "SafeSurfer-Desktop",
  "version": "1.0.0b3",
  "description": "The SafeSurfer desktop app",
  "main": "main.js",
  "license": "GPL-3.0",
  "scripts": {
    "start": "electron .",
    "sscli-node": "node ./assets/scripts/sscli.js",
    "sscli": "bash ./support/linux/shared-resources/sscli",
    "electron-builder": "node_modules/.bin/electron-builder",
    "electron-icon-maker": "node_modules/.bin/electron-icon-maker",
    "package-linux": "node_modules/.bin/electron-packager . SafeSurfer-Desktop --overwrite --asar --platform=linux --arch=x64 --icon=assets/media/icons/png/2000x2000.png --prun=true --out=release-builds",
    "package-linux32": "node_modules/.bin/electron-packager . SafeSurfer-Desktop --overwrite --asar --platform=linux --arch=ia32 --icon=assets/media/icons/png/2000x2000.png --prun=true --out=release-builds",
    "package-macos": "node_modules/.bin/electron-packager . --overwrite --platform=darwin --arch=x64 --prune --out=release-builds",
    "package-win": "node_modules/.bin/electron-packager . SafeSurfer-Desktop --overwrite --asar --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"SafeSurfer-Desktop\"",
    "package-win32": "node_modules/.bin/electron-packager . SafeSurfer-Desktop --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"SafeSurfer-Desktop\"",
    "compile-win-setup": "node ./support/windows/compile.js ./support/windows/inno-script/build-windows-installer.iss",
    "compile-win-setup32": "node ./support/windows/compile.js ./support/windows/inno-script/build-windows-installer32.iss",
    "lintTranslations": "node ./unitTests/lintTranslations.js"
  },
  "repository": "https://gitlab.com/safesurfer/SafeSurfer-Desktop",
  "keywords": [
    "SafeSurfer",
    "safe surfer",
    "safe",
    "surfer"
  ],
  "author": "Safe Surfer",
  "devDependencies": {
    "electron": "^2.0.8",
    "electron-builder": "^20.28.2",
    "electron-icon-maker": "0.0.4",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^12.1.1"
  },
  "dependencies": {
    "bonjour": "^3.5.0",
    "electron-is-dev": "^1.0.1",
    "electron-store": "^2.0.0",
    "electron-window-state": "^5.0.1",
    "electron-windows-store": "^2.0.0",
    "innosetup-compiler": "^5.5.9",
    "is-admin": "^2.1.1",
    "is-online": "^7.0.0",
    "jquery": "^3.3.1",
    "lets-get-meta": "^2.1.1",
    "moment": "^2.22.2",
    "node-dns-sd": "^0.2.0",
    "node_dns_changer": "^1.0.6",
    "nodejs-base64-encode": "0.0.2",
    "npm": "^6.4.0",
    "os-locale": "^3.0.0",
    "reboot": "^0.1.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "shelljs": "^0.8.2",
    "store": "^2.0.12"
  }
}
